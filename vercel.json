{
  "version": 2,
  "builds": [
    {
      "src": "fb-form/src/server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "fb-form/dist/fb-form/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "fb-form/src/server.ts"
    }
  ]
}
